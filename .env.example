# Database
DATABASE_URL="postgresql://user:password@localhost:5432/ts-connect"

# Authentication (Better Auth)
BETTER_AUTH_SECRET="your-secret-at-least-32-characters"
BETTER_AUTH_URL="http://localhost:3001"
CORS_ORIGIN="http://localhost:3001"

# Feature Flags
# Set to false to disable authentication for development/testing
ENABLE_AUTH=true

# AI
ENABLE_AI_COACH=false
AI_PROVIDER="openai"
AI_MODEL="gpt-4o-mini"
AI_MAX_INPUT_CHARS=8000
AI_MAX_MESSAGES=24
OPENAI_API_KEY=""
ANTHROPIC_API_KEY=""

# Environment
NODE_ENV=development
